{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  
  "extends": [
    "config:recommended"
  ],
  "dependencyDashboard": true,
  "automerge": false,


  "schedule": ["at 9am on monday"],


  "labels": [
    "dependency",
    "renovate"
  ],

"rebaseWhen": "never",

  "reviewers": ["suke-0612"],

  "rebaseWhen": "never",


  
  "major": {
    "enabled": false
  },
  "commitMessageExtra": "",


  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "enabled": false
    },
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "enabled": true,
      "groupName": "react",
      "matchPackageNames": ["@types/react", "@types/react-dom"],
      "matchPackagePrefixes": ["react"]
    },
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "enabled": true,
      "groupName": "eslint",
      "matchPackageNames": ["@types/eslint", "babel-eslint"],
      "matchPackagePrefixes": ["@typescript-eslint/", "eslint"]
    },
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "enabled": true,
      "matchPackagePatterns": ["nx"],
      "groupName": "nx"
    },
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "enabled": true,
      "matchPackagePatterns": ["knip"],
      "groupName": "knip"
    },
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "enabled": true,
      "matchPackagePatterns": ["slate","classnames"],
      "groupName": "tekitou"
    }
  ]
}